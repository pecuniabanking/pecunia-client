          <DATA>
            <ELEM name="data" type="bytes" />
          </DATA>
        </send>
        <results>
          <result sw1="0x61" type="success" >
            Command executed, correct size in SW2
          </result>
          <result sw1="0x69" sw2="0x86" type="error" >
            No EF selected (internal error)
          </result>
        </results>
      </command>

